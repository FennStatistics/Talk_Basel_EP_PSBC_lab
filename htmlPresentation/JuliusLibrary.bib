@article{muidermanFourApproachesAnticipatory2020,
  title = {Four Approaches to Anticipatory Climate Governance: {{Different}} Conceptions of the Future and Implications for the Present},
  shorttitle = {Four Approaches to Anticipatory Climate Governance},
  author = {Muiderman, Karlijn and Gupta, Aarti and Vervoort, Joost and Biermann, Frank},
