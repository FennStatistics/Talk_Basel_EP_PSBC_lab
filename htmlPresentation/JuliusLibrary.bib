@article{poppingaPlantMovementsConcept2020,
  title = {Plant {{Movements}} as {{Concept Generators}} for the {{Development}} of {{Biomimetic Compliant Mechanisms}}},
  author = {Poppinga, Simon and Correa, David and Bruchmann, Bernd and Menges, Achim and Speck, Thomas},
