@article{converseIfHumansDesign2021,
  title = {If Humans Design the Planet: {{A}} Call for Psychological Scientists to Engage with Climate Engineering},
  shorttitle = {If Humans Design the Planet},
  author = {Converse, Benjamin A. and Hancock, Patrick I. and Klotz, Leidy E. and Clarens, Andres F. and Adams, Gabrielle S.},
