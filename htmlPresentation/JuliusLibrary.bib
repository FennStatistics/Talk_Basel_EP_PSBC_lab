@article{pallentinConstructionValidationHeiQ2023,
  title = {Construction and {{Validation}} of the {{HeiQ}}: {{An Operation-Oriented Figural Matrices Test}}},
  shorttitle = {Construction and {{Validation}} of the {{HeiQ}}},
  author = {Pallentin, Vanessa S. and Danner, Daniel and Rummel, Jan},
  year = {2023},
