  file = {C\:\\Users\\fenn\\Zotero\\storage\\JMLRNPLT\\Donella H. Meadows, Jorgen Randers, Dennis L. Meadows - Limits to Growth_ The 30-Year Update -Earthscan (2004).pdf;C\:\\Users\\fenn\\Zotero\\storage\\M2MAZP6F\\Meadows and Randers - 2004 - The Limits to Growth The 30-year Update.pdf}
}

@techreport{meadowsLimitsGrowthClub1972,
