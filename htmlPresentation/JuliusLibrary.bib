@article{jewellPoliticalFeasibilityClimate2020,
  title = {On the Political Feasibility of Climate Change Mitigation Pathways: {{Is}} It Too Late to Keep Warming below 1.5\textdegree{{C}}?},
  shorttitle = {On the Political Feasibility of Climate Change Mitigation Pathways},
  author = {Jewell, Jessica and Cherp, Aleh},
