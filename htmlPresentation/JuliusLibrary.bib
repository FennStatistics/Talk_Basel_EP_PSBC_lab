@phdthesis{sendtnerKostbareKistenGrunde2021,
  title = {Kostbare {{Kisten}}: {{Gr\"unde}} F\"ur {{Fehleinsch\"atzungen}} Der {{Kosten}} Des Eigenen {{Autos}} Und Deren {{Auswirkungen}} Auf Die {{Bewertung}} Des {{\"OPNV}} -{{Masterarbeit}}},
