  author = {Legault, Georges-Auguste and B{\'e}land, Jean-Pierre and Parent, Monelle and {K.-B{\'e}dard}, Suzanne and Bellemare, Christian A. and Bernier, Louise and Dagenais, Pierre and Daniel, Charles-{\'E}tienne and Gagnon, Hubert and Patenaude, Johane},
  year = {2019},
