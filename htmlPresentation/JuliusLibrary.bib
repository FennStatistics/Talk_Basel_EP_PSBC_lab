@article{langhannsSelforganizedTaskSwitching2021,
  title = {The Self-Organized Task Switching Paradigm: {{Movement}} Effort Matters},
  shorttitle = {The Self-Organized Task Switching Paradigm},
  author = {Langhanns, Christine and Monno, Irina and Maurer, Heiko and Ebel, Juliane and M{\"u}ller, Hermann and Kiesel, Andrea},
