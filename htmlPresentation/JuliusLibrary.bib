@article{vanasseltUncertaintyIntegratedAssessment2002,
  title = {Uncertainty in {{Integrated Assessment Modelling}}},
  author = {{van Asselt}, Marjolein B. A. and Rotmans, Jan},
  year = {2002},
  month = jul,
  journal = {Climatic Change},
  volume = {54},
