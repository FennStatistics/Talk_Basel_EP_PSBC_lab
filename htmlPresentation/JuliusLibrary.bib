  keywords = {basic research,Grundlagenforschung,Nachhaltigkeit,neue Technologie,new technology,sustainability,technical development,Technikfolgenabsch\"atzung,technische Entwicklung,technology assessment,transdisciplinary,Transdisziplinarit\"at},
